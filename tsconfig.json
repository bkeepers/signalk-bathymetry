{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "resolveJsonModule": true,
    "sourceMap": true,
    "tsBuildInfoFile": "dist/.tsbuildinfo",
    "rootDir": "src",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts"] /* Specify files to be included in the program. */
}
